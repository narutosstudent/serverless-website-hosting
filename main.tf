terraform {

  cloud {
    organization = "tiger_projects"
    workspaces {
      name = "serverless-website-hosting"
    }
  }

  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 4.16"
    }
  }

  required_version = ">= 1.2.0"
}

provider "aws" {
  region = "us-west-2"
}
